<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.choat.plan.postgre.mybatisplus.dao.IPostgresUserDao">
    <select id="findUserList" resultType="com.choat.plan.postgre.mybatisplus.entity.PostgresUserEntity">
        select
        user_id ,
        user_name ,
        email ,
        age ,
        created_by ,
        creation_date ,
        last_update_by ,
        last_update_date
        from
        user_t ut
    </select>
</mapper>